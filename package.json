  "devDependencies": {
    "@types/bluebird": "^3.5.26",
    "@types/express": "^4.16.1",
    "@types/joi": "^14.3.2",
    "@types/mocha": "^5.2.6",
    "@types/node": "^12.0.0",
    "apidoc": "^0.17.7",
    "codacy-coverage": "^3.4.0",
    "coveralls": "^3.0.3",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "forever": "^0.15.3",
    "nodemon": "^1.18.10",
    "nyc": "^13.3.0",
    "opn-cli": "^4.0.0",
